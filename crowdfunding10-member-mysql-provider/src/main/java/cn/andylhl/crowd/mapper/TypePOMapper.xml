    保存项目id与分类id之间的关系
    void saveTypeList(Map<String, String> typeMap, String projectid);
  -->
  <insert id="saveTypeList" parameterType="String">
    insert into t_project_type
    (
     id,
     projectid,
     typeid
    )
    values
    <foreach collection="typeMap.entrySet()"  separator=","  index="key" item="val">
      (#{key}, #{projectid}, #{val})
    </foreach>
  </insert>


